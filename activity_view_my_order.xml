<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/view"
    tools:context=".ViewMyOrder">

    <TextView
        android:id="@+id/viewMyOrder"
        android:layout_width="422dp"
        android:layout_height="153dp"
        android:layout_marginTop="40dp"
        android:fontFamily="cursive"
        android:text="Do You know what have you ordered till now??"
        android:textColor="#0B0A0A"
        android:textSize="35dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="422dp"
        android:layout_height="204dp"
        android:fontFamily="cursive"
        android:text="Click View My Orders !!!"
        android:textColor="#101010"
        android:textSize="35dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewMyOrder" />

    <Button
        android:id="@+id/btnViewAll"
        android:layout_width="420dp"
        android:layout_height="94dp"
        android:layout_marginBottom="276dp"
        android:background="#D2FFFFFF"
        android:fontFamily="cursive"
        android:text="View My Orders"
        android:textColor="#0E0E0E"
        android:textSize="35dp"
        android:textStyle="bold"
        app:backgroundTint="#D2FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>